<div class=bodypanel>
	<center><span class=lbl2>Grades Completion Report</span></center>
	<table class=bodypanel>
		<thead>
			<tr>
				<th>Teacher</th>
				<th>Course</th>
				<th>Stu<br />Count</th>
				<th style="border-left: 5px solid #567;">Q1m<br />Count</th>
				<th>Q1m<br />Min</th>
				<th>Q1m<br />Max</th>
				<th>Q1m<br />Avg</th>
				<th style="border-left: 5px solid #567;">Q1e<br />Count</th>
				<th>Q1e<br />Min</th>
				<th>Q1e<br />Max</th>
				<th>Q1e<br />Avg</th>
				<th style="border-left: 5px solid #567;">Q2m<br />Count</th>
				<th>Q2m<br />Min</th>
				<th>Q2m<br />Max</th>
				<th>Q2m<br />Avg</th>
				<th style="border-left: 5px solid #567;">Q2e<br />Count</th>
				<th>Q2e<br />Min</th>
				<th>Q2e<br />Max</th>
				<th>Q2e<br />Avg</th>
			</tr>
		</thead>
		<tbody>
			<tr id=row>
				<td>{teacherName}</td>
				<td>{courseName}</td>
				<td>{countStudents}</td>
				<td style="border-left: 5px solid #567;">{countq1mScores}</td>
				<td>{minq1mScore}</td>
				<td>{maxq1mScore}</td>
				<td>{avgq1mScore}</td>
				<td style="border-left: 5px solid #567;">{countq1eScores}</td>
				<td>{minq1eScore}</td>
				<td>{maxq1eScore}</td>
				<td>{avgq1eScore}</td>
				<td style="border-left: 5px solid #567;">{countq2mScores}</td>
				<td>{minq2mScore}</td>
				<td>{maxq2mScore}</td>
				<td>{avgq2mScore}</td>
				<td style="border-left: 5px solid #567;">{countq2eScores}</td>
				<td>{minq2eScore}</td>
				<td>{maxq2eScore}</td>
				<td>{avgq2eScore}</td>
			</tr>
		</tbody>
	</table>
</div>