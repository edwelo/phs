<div class=flbeg>
	<div class=bodypanel>
		<span class=lbl1>EDIT BIO DATA</span><br />
		<span class=lbl1>First Name</span><br />
		<input type=text name=firstName value="{firstName}" size=40 /><br />
		<span class=lbl1>Middle Name</span><br />
		<input type=text name=middleName value="{middleName}" size=40 /><br />
		<span class=lbl1>Last Name</span><br />
		<input type=text name=lastName value="{lastName}" size=40 /><br />
		<span class=lbl1>Name Suffix</span><br />
		<input type=text name=suffix value="{suffix}" size=40 /><br />
		<span class=lbl1>Gender</span><br />
		<input type=text name=gender value="{gender}" size=40 /><br />
		<span class=lbl1>Hospital Number</span><br />
		<input type=text name=hospitalNum value="{hospitalNum}" size=40 /><br />
		<span class=lbl1>Date of Birth</span><br />
		<input type=text name=birthDate value="{birthDate}" size=40 /><br />
		<span class=lbl1>Place of Birth</span><br />
		<input type=text name=birthPlace value="{birthPlace}" size=40 /><br />
		<span class=lbl1>Citizenship</span><br />
		<input type=text name=citizenship value="{citizenship}" size=40 /><br />
		<span class=lbl1>Ethnicity</span><br />
		<input type=text name=ethnicity value="{ethnicity}" size=40 /><br />
		<span class=lbl1>Social Security</span><br />
		<input type=text name=ss value="{ss}" size=40 /><br />
		<span class=lbl1>Current Residence</span><br />
		<input type=text name=placeResidence value="{placeResidence}" size=40 /><br />
		<span class=lbl1>Permanent Residence</span><br />
		<input type=text name=permResidence value="{permResidence}" size=40 /><br />
		<span class=lbl1>Elementary School</span><br />
		<input type=text name=elementarySch value="{elementarySch}" size=40 /><br />
		<span class=lbl1>Counselor</span><br />
		<input type=text name=counselor value="{counselor}" size=40 /><br />
		<span class=lbl1>Advisor</span><br />
		<input type=text name=advisor value="{advisor}" size=40 /><br />
		<span class=lbl1>Record Date</span><br />
		<input type=text name=date value="{date}" size=40 /><br />
		<span class=lbl1>Notes</span><br />
		<input type=text name=notes value="{notes}" size=40 /><br />
	</div>
</div>

